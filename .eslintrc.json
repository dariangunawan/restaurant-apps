{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "eol-last":"off",
        "linebreak-style": 0,
        "indent": "off",
        "no-console": 0,
        "no-extra-semi": 0,
        "import/no-extraneous-dependencies": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "class-methods-use-this":"off"
    }
}